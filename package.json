{
  "name": "statflix",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers Chrome",
    "test:coverage": "ng test --browsers Chrome --no-watch --code-coverage",
    "ci:test": "ng test --browsers ChromeHeadless --no-watch",
    "ci:test:coverage": "ng test --browsers ChromeHeadless --no-watch --code-coverage",
    "lint": "ng lint",
    "prettier": "prettier 'src/**/*' --ignore-unknown --write",
    "postinstall": "husky install"
  },
  "private": true,
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "bracketSpacing": true
  },
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "14.2.2",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/material": "14.2.2",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@angular/service-worker": "^14.1.0",
    "@ngrx/store": "^14.3.1",
    "@swimlane/ngx-charts": "^20.1.0",
    "fast-sort": "^3.2.0",
    "fuse.js": "^6.6.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular-eslint/builder": "14.1.1",
    "@angular-eslint/eslint-plugin": "14.1.1",
    "@angular-eslint/eslint-plugin-template": "14.1.1",
    "@angular-eslint/schematics": "14.1.1",
    "@angular-eslint/template-parser": "14.1.1",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@ngrx/eslint-plugin": "^14.3.1",
    "@types/d3": "^7.4.0",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "d3": "^7.4.0",
    "eslint": "^8.23.1",
    "husky": "^8.0.1",
    "jasmine-core": "~4.2.0",
    "jasmine-marbles": "^0.9.2",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "prettier": "^2.7.1",
    "typescript": "~4.7.2"
  }
}
